  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-img": "^0.10.0",
    "debug": "^4.2.0",
    "html-minifier": "^4.0.0",
    "json-to-scss": "^1.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "lite-youtube-embed": "^0.2.0",
    "sass": "^1.39.0"
  }
